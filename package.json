{
  "name": "kempo-server",
  "type": "module",
  "version": "1.7.6",
  "description": "A lightweight, zero-dependency, file based routing server.",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./utils/cli": "./dist/utils/cli.js",
    "./utils/fs-utils": "./dist/utils/fs-utils.js"
  },
  "bin": {
    "kempo-server": "./dist/index.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "docs": "node dist/index.js -r ./docs",
    "tests": "npx kempo-test",
    "tests:gui": "npx kempo-test --gui",
    "tests:browser": "npx kempo-test -b",
    "tests:node": "npx kempo-test -n"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "essentialcss": "^2.0.1",
    "kempo-testing-framework": "^1.2.3",
    "terser": "^5.43.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinpoissant/kempo-server"
  },
  "dependencies": {
    "kempo-css": "^1.0.5"
  }
}
